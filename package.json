{
  "name": "work-calendar",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "main": "dist/entry.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:ng\" \"npm run start:electron\" \"npm run start:server\"",
    "start:ng": "ng serve --proxy-config proxy.conf.json",
    "start:server": "json-server --watch db.json",
    "start:electron": "electron src/electron/entry.live.js",
    "build:production": "ng build --prod --aot",
    "build:electron": "npm run build:production && cp src/electron/* dist/",
    "electron": "npm run build:electron && electron dist/",
    "package:linux": "electron-packager dist/ electron-explorer-$npm_package_version --out=packages --platform=linux --arch=all --overwrite",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/flex-layout": "^2.0.0-beta.5",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "angular2-fullcalendar": "https://github.com/lbertenasco/ng2-fullcalendar.git",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "material-design-colors": "^1.0.2",
    "nedb": "^1.8.0",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.31",
    "@angular/compiler-cli": "^2.4.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.38",
    "@types/nedb": "^0.0.31",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.3.0",
    "electron": "^1.4.15",
    "electron-packager": "^8.5.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "json-server": "^0.9.5",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.0.2",
    "protractor": "~5.1.0",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.0"
  }
}
